{
  "name": "@do-while-for-each/bundler",
  "description": "The Bundler of JS/TS sources based on webpack",
  "version": "1.2.2",
  "homepage": "https://github.com/dwfe/bundler",
  "bugs": "https://github.com/dwfe/bundler/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:dwfe/bundler.git"
  },
  "keywords": [
    "bundler",
    "bundle",
    "webpack"
  ],
  "author": "Do|While|For|Each",
  "license": "MIT",
  "bin": {
    "dwfeBundler": "dist/bin.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "dwfeFs cleanDir . --fileNames=\"dist;node_modules;package-lock.json\"",
    "build": "dwfeFs rmForce dist --noConfirm && tsc -b src/tsconfig.json",
    "version": "npm run build",
    "postversion": "git push",
    "deploy": "npm version patch && npm publish"
  },
  "dependencies": {
    "webpack": "5.72.1",
    "webpack-merge": "5.8.0",
    "webpack-dev-server": "4.9.0",
    "html-webpack-plugin": "5.5.0",
    "ts-loader": "9.3.0",
    "style-loader": "3.3.1",
    "css-loader": "6.7.1",
    "less-loader": "10.2.0",
    "postcss": "8.4.13",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.5.0",
    "postcss-normalize": "10.0.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "mini-css-extract-plugin": "2.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "resolve-url-loader": "5.0.0",
    "@do-while-for-each/log-node": "^1.0.9",
    "@do-while-for-each/env": "^1.0.6",
    "@do-while-for-each/fs": "^1.2.21"
  },
  "devDependencies": {
    "@do-while-for-each/tsconfig": "^1.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
